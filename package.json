  "homepage": "https://github.com/nickwillan/cdk-encrypted-secret#readme",
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.2",
    "aws-cdk-lib": "^2.145.0",
    "aws-sdk-client-mock": "^4.0.1",
    "esbuild": "^0.21.4",
    "eslint": "^9.4.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "prettier": "^3.3.1",
    "ts-jest": "^29.1.4",
    "typescript": "^5.4.5"
  },
  "peerDependencies": {
    "aws-cdk-lib": "^2.130.0",
    "constructs": "^10.3.0"
  },
  "dependencies": {
    "@aws-sdk/client-kms": "^3.592.0",
    "@aws-sdk/client-secrets-manager": "^3.592.0",
    "@aws-sdk/util-arn-parser": "^3.568.0"
  }
}
